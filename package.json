{
  "name": "hub-iveco",
  "version": "1.0.0",
  "description": "Hub-Iveco Project",
  "scripts": {
    "build": "node_modules/typescript/bin/tsc",
    "watch": "node_modules/typescript/bin/tsc -w --preserveWatchOutput",
    "test": "jest --coverage"
  },
  "dependencies": {
    "aws-sdk": "^2.976.0",
    "class-transformer": "^0.4.0",
    "dotenv": "^10.0.0",
    "inversify": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mysql2": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.9.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.83",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.17.12",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "testTimeout": 30000,
    "setupFiles": [
      "dotenv/config"
    ]
  }
}
